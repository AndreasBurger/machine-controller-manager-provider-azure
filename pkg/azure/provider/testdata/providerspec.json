{
  "location": "westeurope",
  "properties": {
    "hardwareProfile": {
      "vmSize": "Standard_DS2_v2"
    },
    "networkProfile": {
      "networkInterfaces": {},
      "acceleratedNetworking": false
    },
    "osProfile": {
      "adminUsername": "core",
      "customData": "<string>",
